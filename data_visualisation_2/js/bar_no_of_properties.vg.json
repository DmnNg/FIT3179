{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "No. of Properties Sold by Region",
  "data": {"url": "https://raw.githubusercontent.com/DmnNg/FIT3179/main/data_visualisation_2/data/clean_melbourne_housing.csv"},
  "mark": "bar",
  "encoding": {
    "y": {
      "field": "RegionName",
      "type": "nominal",
      "sort": "-x"
    },
    "x": {
      "aggregate": "count",
      "type": "quantitative"
    },
    "tooltip": [
      {"field": "RegionName", "type": "nominal"},
      {"aggregate": "count",
      "type": "quantitative"}
      ]
  }
}
